FROM fedora:27

RUN dnf -y update && dnf -y install \
    autoconf \
    antlr3-C-devel \
    automake \
    cmake \
    findutils \
    gcc-arm-linux-gnu \
    gcc-c++ \
    java-1.8.0-openjdk-headless \
    mercurial \
    make \
    ncurses-devel \
    openssh-clients \
    wget \
    zlib-devel \
    \
    && dnf clean all

